composer-rest-server -c admin@land-agreement -n never -w true

 composer network install --card PeerAdmin@hlfv1 --archiveFile land-agreement@0.0.1.bna

 composer network start --networkName land-agreement --networkVersion 0.0.1 --networkAdmin admin --networkAdminEnrollSecret adminpw --card PeerAdmin@hlfv1 --file networkadmin.card


composer network ping --card admin@land-agreement
